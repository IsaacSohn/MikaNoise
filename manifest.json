{
    "manifest_version": 3,
    "name": "Mika Noise",
    "version": "1.0",
    "icons": {
        "128": "assets/icon/icon-128.png"
      },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "alarms",
      "offscreen"
    ],
    "web_accessible_resources": [{
        "resources":["audio.mp3"],
        "matches": ["<all_urls>"]
    }]
  }
  